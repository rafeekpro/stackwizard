{
  "name": "@rafeekpro/stackwizard",
  "version": "1.1.0",
  "description": "🧙‍♂️ Magical full-stack project generator with FastAPI, React, PostgreSQL, and Docker",
  "main": "src/index.js",
  "bin": {
    "stackwizard": "./src/index.js",
    "create-fullstack-app": "./src/index.js"
  },
  "files": [
    "src/",
    "templates/",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "start": "node src/index.js",
    "test": "node --test src/**/*.test.js || echo 'No tests found'",
    "test:package": "node test/test-npm-package-files.js",
    "test:structure": "node test/test-generated-structure.js",
    "test:docker": "node test/test-docker-compose.js",
    "test:errors": "jest test/error-handling.test.js --verbose",
    "test:all": "npm run test:package && npm run test:structure && npm run test:errors",
    "test:ci": "npm run test:package && npm run test:structure",
    "lint": "eslint src/ --ext .js",
    "format": "prettier --write 'src/**/*.js'",
    "format:check": "prettier --check 'src/**/*.js'",
    "prepublishOnly": "npm run lint && npm run format:check && npm run test:package"
  },
  "keywords": [
    "generator",
    "boilerplate",
    "fullstack",
    "full-stack",
    "react",
    "fastapi",
    "docker",
    "docker-compose",
    "postgresql",
    "material-ui",
    "mui",
    "tailwindcss",
    "tailwind",
    "cli",
    "create-app",
    "scaffold",
    "starter",
    "template",
    "alembic",
    "sqlalchemy",
    "jwt",
    "authentication"
  ],
  "author": {
    "name": "StackWizard Team",
    "email": "support@stackwizard.dev",
    "url": "https://github.com/rafeekpro"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafeekpro/stackwizard.git"
  },
  "bugs": {
    "url": "https://github.com/rafeekpro/stackwizard/issues"
  },
  "homepage": "https://github.com/rafeekpro/stackwizard#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "publishConfigGitHub": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "type": "module",
  "devDependencies": {
    "axios": "^1.11.0",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2",
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0"
  }
}
